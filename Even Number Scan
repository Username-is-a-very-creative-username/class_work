using System;

class Program {
  public static void Main (string[] args) {
int i;
    Console.WriteLine("enter the number");
    i=Convert.ToInt32(Console.ReadLine());
    
    if (i%2==0)
    {
      Console.WriteLine ("Even Numbers");
    }
    else
    {
    Console.WriteLine("Odd Numebr");
  
}
    
  }
}
